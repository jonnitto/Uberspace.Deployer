{
    "name": "jonnitto/uberspace-deployer",
    "description": "Unofficial deployer recipes for Uberspace interacting with Neos CMS",
    "type": "neos-build",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-intl": "*",
        "deployer/deployer": "^6.8 || ^7.0"
    },
    "archive": {
        "exclude": [
            "/.gitattributes",
            "/.gitignore",
            "/.github",
            "/CODE_OF_CONDUCT.md",
            "/LICENSE"
        ]
    },
    "extra": {
        "marketplace-account": "me@jonnitto.ch",
        "installer-name": "Uberspace.Deployer",
        "neos": {
            "package-key": "Uberspace.Deployer",
            "installer-resource-folders": [
                "/Installer/"
            ]
        }
    }
}
